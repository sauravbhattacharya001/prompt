{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/Prompt.csproj"],
          "src": ".."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appTitle": "Prompt â€” Azure OpenAI Library",
      "_appName": "Prompt",
      "_appFooter": "Made by <a href='https://github.com/sauravbhattacharya001'>Saurav Bhattacharya</a>",
      "_enableSearch": true,
      "_enableNewTab": true
    }
  }
}
